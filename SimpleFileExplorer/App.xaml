<Application x:Class="SimpleFileExplorer.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:SimpleFileExplorer"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/SimpleFileExplorer.Brushes.xaml"/>
                <ResourceDictionary Source="Styles/SimpleFileExplorer.Styles.ListBox.xaml"/>
                <ResourceDictionary Source="Styles/SimpleFileExplorer.Styles.Image.xaml"/>
                <ResourceDictionary Source="Styles/SimpleFileExplorer.Styles.Button.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
